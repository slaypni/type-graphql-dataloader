  "version": "0.3.7",
  "author": {
    "name": "Kazuaki Tanida",
    "url": "https://github.com/slaypni"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/slaypni/type-graphql-dataloader.git"
  },
  "keywords": [
    "typescript",
    "graphql",
    "dataloader",
    "typeorm"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slaypni/type-graphql-dataloader/issues"
  },
  "main": "dist/index.js",
